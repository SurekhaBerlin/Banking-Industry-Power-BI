{
  "name": "Certificate Style Theme",
  "dataColors": ["#FFD700", "#1C1C1C", "#A9A9A9", "#FFFFFF", "#3B3B3B"],
  "background": "#0B0B0D",
  "foreground": "#FFFFFF",
  "tableAccent": "#FFD700",
  "visualStyles": {
    "*": {
      "*": {
        "title": [
          {
            "color": {
              "solid": {
                "color": "#FFD700"
              }
            },
            "fontSize": 14,
            "fontFamily": "Segoe UI"
          }
        ],
        "labels": [
          {
            "color": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "fontSize": 12
          }
        ]
      }
    }
  }
}
